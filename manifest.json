{
  "manifest_version": 2,
  "name": "Paylocity Hours tracker",
  "description": "This extension will return the amount of hours worked on paylocity",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}